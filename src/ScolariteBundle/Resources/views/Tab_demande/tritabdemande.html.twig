<script>
$(document).ready(function() {
            $("#demande").on('keypress', function(e) {
               
 
                var demande = $("#demande").val();
                
                $.ajax({
                    url: '{{path("scolariteSearchDemande")}}',
                    type: 'POST',
//                    dataType: 'html',
                    dataType: 'json',
                    data : { "demande" : demande },
                    success: function(response) {
                        console.log('Data ', response);
                         $("#demandes").empty();
                          $.each(response, function(i, response) {
                              
                            $("#demandes").append(

                                "<tr><td>" + response.Nom_Matiere + "</td>"+
                                "<td>" + response.Coef_Matiere + "</td>"+
                                "<td>" + response.Nbre_Heure_Matiere + "</td>"+
                                "<td>" + response.menseignant+ "</td>"+
                                "<td><a href='/supprimerMatiere/"+response.Id_Matiere +"' ><button class='btn btn-danger'> <i class='fa fa-trash'></i> Supprimer </button></a></td>"+
                                "<td><a href='/modifierMatiere/"+response.Id_Matiere +"' ><button class='btn btn-info'> <i class='fa fa-edit'></i> Modifier</button></a></td></tr>"
                                
                            
                            )
                        });
                    },
                    error: function(error) {
                        console.log(error)
                    }
                })
 
            })
        });
</script>

