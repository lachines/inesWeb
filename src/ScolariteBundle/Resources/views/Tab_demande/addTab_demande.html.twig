<section class="login_part section_padding ">
 <div class="container">
  <div class="row align-items-center">
   <div class="col-lg-12 col-md-12">
    <div class="">
    {% if first == 0 %}
    {{ form_start(form) }}
    <div class="my-custom-class-for-errors">
        {{ form_errors(form) }}
    </div>

   
        <div class="form-group">
            {{ form_row(form.nomDemande) }}
        </div>
        <div  class="form-group">
            {{ form_row(form.prenomDemande) }}
        </div>
         <div class="form-group">
            {{ form_row(form.cinDemande) }}
        </div>
        
        <div  class="form-group">
            {{ form_row(form.numTelDemande) }}
        </div>

        <div class="form-group">
            {{ form_row(form.cVDemande) }}
        </div>

        <div  class="form-group">
        {{ form_row(form.dateNaissance) }}
        </div> 

         <div class="form-group">
            {{ form_row(form.etudeDemande) }}
        </div>

         <div class="form-group">
            {{ form_row(form.posteDemande) }}
        </div>

        


    <div >

  {{ form_end(form) }}
{% else %}
<div class="alert alert-success">
  Vous avez envoye deja une demande d'embauche ...!!

</div>
{% endif %}
    </div>
